#include<stdio.h>
#include<conio.h>
int main()
{
	int n,rem,sum=0,temp;
	printf("Enter the number:");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
		rem=n%10;
		sum=sum+(rem*rem*rem);
		n=n/10;
	}
	if(sum==temp)
	 printf("\nGiven number is armstrong");
	else
	 printf("\nGiven number is not an armstrong");
	getch();
	return 0;
}
